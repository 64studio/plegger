#!/usr/bin/make -f
%:
	dh $@ --with=systemd

override_dh_systemd_enable:
	dh_systemd_enable plegger.recorder.service
	dh_systemd_enable plegger.webapp.service

override_dh_installinit:
	dh_installinit --name=recorder
	dh_installinit --name=webapp

override_dh_systemd_start:
	dh_systemd_start plegger.recorder.service
	dh_systemd_start plegger.webapp.service
